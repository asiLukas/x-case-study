<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="hr_applicant_view_form" model="ir.ui.view">
    <field name="name">hr.applicant.form.inherit</field>
    <field name="model">hr.applicant</field>
    <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_form" />
    <field name="arch" type="xml">
      <xpath expr="//field[@name='email_from']" position="before">
        <field name="applicant_number" string="Applicant Number" />
      </xpath>
    </field>
  </record>
  <menuitem id="menu_custom" name="Custom Menu" sequence="10" />
</odoo>
